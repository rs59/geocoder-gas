{
  "include": ["./src/geocoder_library.ts"],
  "exclude": ["./node_modules"],
  "compilerOptions": {
    "noEmit": true,
    "target": "ES6",
    "noImplicitAny": false,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true
  }
}
